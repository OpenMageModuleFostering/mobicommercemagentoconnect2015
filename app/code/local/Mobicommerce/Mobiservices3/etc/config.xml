<?xml version="1.0"?>
<config>
    <modules>
        <Mobicommerce_Mobiservices3>
            <version>3.0.0</version>
        </Mobicommerce_Mobiservices3>
    </modules>
    <frontend>
        <routers>
            <mobiservices3>
                <use>standard</use>
                <args>
                    <module>Mobicommerce_Mobiservices3</module>
                    <frontName>mobiservices3</frontName>
                </args>
            </mobiservices3>
        </routers>
    </frontend>    
    <global>
        <models>
            <mobiservices3>
                <class>Mobicommerce_Mobiservices3_Model</class>
                <resourceModel>mobiservices3_mysql4</resourceModel>
            </mobiservices3>
            <mobiservices3_mysql4>
                <class>Mobicommerce_Mobiservices3_Model_Mysql4</class>
                <entities>
                    <mobiservices3>
                        <table>mobicommerce_applications2</table>
                    </mobiservices3>
                </entities>
            </mobiservices3_mysql4>
        </models>
        <helpers>
            <mobiservices3>
                <class>Mobicommerce_Mobiservices3_Helper</class>
            </mobiservices3>
        </helpers> 
        <blocks>
            <mobiservices3>
                <class>Mobicommerce_Mobiservices3_Block</class>
            </mobiservices3>
        </blocks>
		<resources>
            <mobiservices3_setup>
                <setup>
                    <module>Mobicommerce_Mobiservices3</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </mobiservices3_setup>
            <mobiservices3_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </mobiservices3_write>
            <mobiservices3_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </mobiservices3_read>
        </resources>
    </global>

    <default>
        <mobicommerce3_config>
            <cache>
                <is_active>0</is_active>
            </cache>
        </mobicommerce3_config>
    </default>   
</config>